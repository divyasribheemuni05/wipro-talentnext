package java_fundamentasArrays;
import java.util.*;
public class Question14 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner s= new Scanner(System.in);
		int n=s.nextInt();
		int arr[]=new int[n];
		if(n==9) {
			for(int i=0;i<n;i++) {
				arr[i]=s.nextInt();
			}
			int _3D[][][]=new int[3][3][3];
			int p=0;
			for(int i=0;i<3;i++) {
				for(int j=0;j<3;j++) {
					for(int k=0;k<3;k++) {
						_3D[i][j][k]=arr[p];
						p++;
					}
				}
			}
			for(int i=0;i<3;i++) {
				for(int j=0;j<3;j++) {
					for(int k=0;k<3;k++) {
						System.out.print(_3D[i][j][k]+" ");
					}
					System.out.println();
				}
			
			}
			
		}

	}

}

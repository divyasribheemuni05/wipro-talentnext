package java_fundamentasArrays;
import java.util.*;
public class Question5 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner s=new Scanner(System.in);
		int n=s.nextInt();
		int arr[]=new int[n];
		for(int i=0;i<n;i++) {
			arr[i]=s.nextInt();
		}
		int max1,max2,min1,min2;
		if(arr[0]>arr[1]) {
			max1=arr[0];
			max2=arr[1];
			min1=arr[1];
			min2=arr[0];
		}
		else {
			max1=arr[1];
			max2=arr[0];
			min1=arr[0];
			min2=arr[1];
		}
		for(int i=2;i<n;i++) {
			if(arr[i]>max1) {
				max2=max1;
				max1=arr[i];
			}
			else{
				max2=arr[i];
			}
			if(arr[i]<min1) {
				min2=min1;
				min1=arr[i];
			}
			else {
				min2=arr[i];
			}
		}
		System.out.println("Largest: "+max1+ " " +max2);
		System.out.println("Smallest: "+min1+ " "+min2);
	}

}

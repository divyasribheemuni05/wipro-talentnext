package java_fundamentasArrays;
import java.util.*;
public class Question3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner s=new Scanner(System.in);
		int n=s.nextInt();
		int arr[]=new int[n];
		for(int i=0;i<n;i++) {
			arr[i]=s.nextInt();
		}
		int j=s.nextInt();
		int ind=-1;
		for(int i=0;i<n;i++) {
			if(arr[i]==j) {
				ind=i;
			}
		}
		if(ind!=-1) {
			System.out.println(+ind);
		}
		else {
			System.out.println("-1");
		}
		
	}

}



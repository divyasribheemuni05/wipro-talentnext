package java_fundamentasArrays;
import java.util.*;
public class Question10 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner s=new Scanner(System.in);
		int n=s.nextInt();
		int arr[]=new int[n];
		int re[]=new int[n];
		for(int i=0;i<n;i++) {
			arr[i]=s.nextInt();
		}
		int index=0;
		for(int i=0;i<n;i++) {
			if(arr[i]%2 ==0) {
				re[index]=arr[i];
				index++;
			}
		}
		for(int i=0;i<n;i++) {
			if(arr[i]%2!=0) {
				re[index]=arr[i];
				index++;
			}
		}
		for(int i=0;i<n;i++) {
			System.out.print(arr[i]+" ");
		}

	}

}

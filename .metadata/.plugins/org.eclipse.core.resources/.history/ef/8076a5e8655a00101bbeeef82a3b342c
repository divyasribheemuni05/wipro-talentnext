package java_fundamentasArrays;
import java.util.*;
public class Question11 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner s= new Scanner(System.in);
		int n=s.nextInt();
		int arr[]=new int[n];
		for(int i=0;i<n;i++) {
			arr[i]=s.nextInt();
		}
		for(int i=0;i<n;i++) {
			if(arr[i]==1 || arr[i]==4) {
				System.out.println("true");
			}
			else {
				System.out.println("false");
				
			}
		}

	}

}
